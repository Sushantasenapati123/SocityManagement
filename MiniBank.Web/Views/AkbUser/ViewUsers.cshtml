@{ var listdata = ViewBag.Userlist as Task<IEnumerable<Bank.Domain.User.UserEntity>>; }
@{ ViewData["Title"] = "AdminRegistation";
    Layout = "~/Views/Shared/dynamicLayoutManager.cshtml"; }
<style>
    tr {
        text-align: center;
    }
</style>
<div class="page-title">
    <div class="title-details">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><span class="icon-home1"></span></li>
                <li class="breadcrumb-item">Society Management</li>
                <li class="breadcrumb-item active" aria-current="page"><b>AKB USER Registration</b></li>
            </ol>
        </nav>
    </div>
</div>
<div class="row">
    <div class="container">
        <div class="col-md-12 col-sm-12">
            <div class="card">
                <div class="card-header">
                    <ul class="nav nav-tabs nav-fill" role="tablist">
                        <a class="nav-item nav-link " asp-controller="AkbUser" asp-action="Addusers" data-toggle="tooltip" id="AccountDetailsAdd">
                            Add
                        </a>

                        <a class="nav-item nav-link active" data-toggle="tooltip" asp-controller="AkbUser" asp-action="ViewUsers" id="AccountDetailsView">
                            View
                        </a>

                    </ul>
                </div>
                <div class="card-body">
                    <table class="table table-responsive-md table-bordered">
                        <thead>
                            <tr>
                                <th width="5px">Sl no.</th>
                                <th width="5px">Branch</th>
                                <th width="5px">Role</th>
                                <th width="5px">User Id</th>
                                <th width="5px">Status</th>
                                <th width="5px">FromDate</th>
                                <th width="5px">ToDate</th>
                                <th width="5px">Name</th>
                                <th width="5px">Email</th>
                                <th width="5px">Mobile</th>
                                <th width="5px">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{ int count = 1;}
                            @foreach (var items in listdata.Result)
                            {
                <tr>
                    <td>@count</td>
                    <td>@items.Branch_Name</td>
                    <td>@items.ROLE_NAME</td>
                    <td>@items.USER_ID</td>
                    <td>@items.USER_STATUS</td>
                    <td>@items.FROM_DATE</td>
                    <td>@items.TO_DATE</td>
                    <td>@items.USER_NAME</td>
                    <td>@items.USER_EMAIL</td>
                    <td>@items.USER_MOBILE</td>
                    <td><a class="btn btn-outline-secondary" href="/AkbUser/EditUsers?id=@items.ID">Edit</a>|<a class="btn btn-outline-danger" id="txtdelete" href="/AkbUser/DeleteUsers?id=@items.ID">Delete</a></td>
                </tr>
count++;
            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<head>
    <script src="http://code.jquery.com/jquery-latest.js"></script>
</head>


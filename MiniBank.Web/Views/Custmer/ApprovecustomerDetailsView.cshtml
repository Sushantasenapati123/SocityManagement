            typr: "POST",
            contentType: "application/json;charset=UTF-8",
            dataType: "json",
            success: function (result) {
                debugger
                if (result == 12) {
                    swal({
                        title: "Approved Successfully",
                        text: "",
                        icon: "success",
                        button: "oh yes!",
                    }).then((value) => {
                        if (true) {
                            location.href = "/Custmer/ViewPendingCustomerBefore";
                        }
                    })


